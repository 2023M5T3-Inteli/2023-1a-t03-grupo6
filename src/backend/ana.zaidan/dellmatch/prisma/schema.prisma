// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  completeName String
  job String
  area String
  jobType String
  location String
  email String @unique
  dellCurriculum String
  teams String @unique
  technologies String
  favoriteTags String
  photo String

  @@map("users")
}
